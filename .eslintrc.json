{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "plugins": ["prettier", "@babel/plugin-transform-modules-commonjs"],
  "parserOptions": {
    "sourceType": "module"
  }
}